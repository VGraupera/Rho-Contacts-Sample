<div id="pageTitle">
	<h1><%= @contact.first_name%></h1>
</div>

<div id="toolbar">
        <%= link_to "Back", :action => :index %>
        <%= link_to "Edit", :action => :edit, :id => @contact.object %>
</div>


<div id="content">

    <table>
      
      <tr>
    	<td class="itemLabel">FirstName</td>
        <td class="itemValue"><%= @contact.first_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">LastName</td>
        <td class="itemValue"><%= @contact.last_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">EmailAddress</td>
        <td class="itemValue"><%= @contact.email_address%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">BusinessNumber</td>
        <td class="itemValue"><%= @contact.business_number%></td>
      </tr>
      
    </table>
</div>