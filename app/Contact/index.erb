<div class="toolbar">
	<h1>Contacts</h1>
  <a class="button add slideup" target="_webapp" href="/app/Contact/new">+</a>
</div>
<ul id="contacts" class="edgetoedge">
	<% @contacts.each do |x| %>
		<li class="arrow">
		  <a href="<%= url_for :action => :show, :id => x[1]['id']%>" target="_webapp"><%= "#{x[1]['first_name']} #{x[1]['last_name']}" %></a>
		</li>
	<%end%>
</ul>
