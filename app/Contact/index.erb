<div id="pageTitle">
	<h1>Contacts</h1>
</div>

<div id="toolbar">
    <div id="leftItem" class="regularButton"><%= link_to "Home", Rho::RhoConfig.start_path %></div>
    <div id="rightItem" class="regularButton"><%= link_to "New", :controller => :Contact, :action => :new %></div>
</div>

<div id="content">
    <ul>
        <% @contacts.each do |obj| %>
            <li>
                <a href="<%= url_for :action => :show, :id => obj[1]['id'] %>">
                    <span class="title"><%= "#{obj[1]['first_name']} #{obj[1]['last_name']}" %></span>
                    <span class="disclosure_indicator"></span>
                </a>
            </li>
        <% end %>
    </ul>
</div>