<div class="toolbar">
	<h1 id="pageTitle">
		Contacts
	</h1>
</div>

<ul id="contacts" title="Contacts" selected="true">
  	<a class="button right_button" href="Contact/new" target="_self">New</a>
	<% @contacts.each do |x| %>
		<li><%=link_to "#{x[1]['first_name']} #{x[1]['last_name']}", :action => :show, :id => x[1]['id']%></li>
	<%end%>
</ul>